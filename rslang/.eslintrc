{
    "extends": "airbnb-base",
     "env": {
         "browser": true,
        "node": true,
        "es6": true
     },
     "settings": {
      "import/extensions": [
        ".js"
      ]
    },

     "rules":{
        "no-unused-expressions": 0,
         "consistent-return": 0,
         "no-param-reassign": ["error", { "props": false }],
         "import/no-cycle": [2, { "maxDepth": 1 }],
         "import/prefer-default-export": "off",
         "camelcase": ["error", {"properties": "never", "ignoreDestructuring": true}]
        }
}
